    "description": "PHP client for beanstalkd queue",
    "keywords": ["beanstalkd"],
    "homepage": "https://github.com/augustash/pheanstalk",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Annesley",
            "email": "paul@annesley.cc",
            "homepage": "http://paul.annesley.cc/",
            "role": "Developer"
        }
    ],
    "require": {
